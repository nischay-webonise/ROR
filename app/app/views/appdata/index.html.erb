<p id="notice"><%= notice %></p>

<h1>Listing Appdata</h1>

<table>
  <thead>
    <tr>
      <th>app name</th>
      <th>app description</th>
      <th>phone number</th>
      <th>address</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @appdata.each do |appdatum| %>
      <tr>
        <td><%= appdatum.name %></td>
        <td><%= appdatum.description %></td>
        <td><%= appdatum.phone_number %></td>
        <td><%= appdatum.address %></td>
        <td><%= link_to 'Show', appdatum %></td>
        <td><%= link_to 'Edit', edit_appdatum_path(appdatum) %></td>
        <td><%= link_to 'Destroy', appdatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appdatum', new_appdatum_path %>
